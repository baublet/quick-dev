{
  "name": "quick-dev",
  "version": "1.0.0",
  "description": "quick-dev",
  "main": "src/server/index.js",
  "scripts": {
    "db:migration:create": "yarn knex migrate:make -x ts",
    "db:migrate": "yarn ts-node ./node_modules/.bin/knex migrate:latest",
    "db:drop": "ts-node scripts/dropDatabase.ts",
    "db:create": "ts-node scripts/createDatabase.ts",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate",
    "develop:dependencies": "docker-compose up -d",
    "build:netlify": "NODE_ENV=production yarn concurrently npm:build:server npm:build:web npm:build:provisioner",
    "build:server": "node scripts/buildServer.js",
    "build:provisioner": "node scripts/buildProvisioner.js",
    "build:web": "node ./scripts/buildWeb.js",
    "build:graphql": "graphql-codegen",
    "develop": "yarn develop:stop; yarn develop:dependencies; yarn pm2 start ./ecosystem.config.js; yarn develop:logs",
    "develop:logs": "yarn pm2 logs",
    "develop:stop": "yarn pm2 stop ./ecosystem.config.js; yarn pm2 delete all; rm -rf ~/.pm2",
    "develop:reload": "yarn develop:stop; yarn develop",
    "lint": "yarn tsc --noEmit",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baublet/quick-dev.git"
  },
  "author": "Ryan Poe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baublet/quick-dev/issues"
  },
  "homepage": "https://github.com/baublet/quick-dev#readme",
  "dependencies": {
