{
  "name": "quick-dev",
  "version": "1.0.0",
  "description": "quick-dev",
  "main": "src/server/index.js",
  "scripts": {
    "db:migration:create": "yarn knex migrate:make -x ts",
    "db:migrate": "yarn ts-node ./node_modules/.bin/knex migrate:latest",
    "db:reset": "rm -rf strapyard.sqlite && yarn db:migrate",
    "build:netlify": "NODE_ENV=production yarn concurrently npm:build:lambda npm:build:web",
    "build:lambda": "netlify-lambda build -c webpack.functions.js src/lambda",
    "build:web": "webpack-cli -c webpack.web.js",
    "develop:ngrok": "rm -rf .ngrokDomain && node ./ngrok.js",
    "develop:lambda": "yarn netlify dev",
    "develop:workerCaller": "yarn ts-node ./devWorkerCaller.ts",
    "develop:web": "webpack-cli -c webpack.web.js --watch",
    "develop": "yarn concurrently npm:develop:lambda npm:develop:web npm:develop:workerCaller npm:develop:ngrok",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baublet/quick-dev.git"
  },
  "author": "Ryan Poe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baublet/quick-dev/issues"
  },
  "homepage": "https://github.com/baublet/quick-dev#readme",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "apollo-server-lambda": "^2.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "cookie": "^0.4.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.13",
    "glob": "^7.1.6",
    "human-readable-ids": "^1.0.4",
    "knex": "^0.21.14",
    "memory-fs": "^0.5.0",
    "node-fetch": "^3.0.0-beta.9",
    "node-pty": "^0.9.0",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^4.5.0",
    "netlify-cli": "^2.69.11",
    "netlify-lambda": "^2.0.2",
    "ngrok": "^3.4.0",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
