{
  "name": "quick-dev",
  "version": "1.0.0",
  "description": "quick-dev",
  "main": "src/server/index.js",
  "scripts": {
    "db:migration:create": "yarn knex migrate:make -x ts",
    "db:migrate": "yarn ts-node ./node_modules/.bin/knex migrate:latest",
    "build:lambda": "netlify-lambda build src/lambda -c webpack.functions.js",
    "build:web": "webpack-cli -c client.webpack.config.js",
    "develop:lambda": "yarn netlify dev",
    "develop:web": "webpack-cli -c client.webpack.config.js --watch",
    "develop": "yarn concurrently npm:develop:lambda npm:develop:web",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baublet/quick-dev.git"
  },
