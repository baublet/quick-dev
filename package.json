{
  "name": "quick-dev",
  "version": "1.0.0",
  "description": "quick-dev",
  "main": "src/server/index.js",
  "scripts": {
    "db:migration:create": "yarn knex migrate:make -x ts",
    "db:migrate": "yarn ts-node ./node_modules/.bin/knex migrate:latest",
    "db:reset": "rm -rf strapyard.sqlite && yarn db:migrate",
    "build:netlify": "NODE_ENV=production yarn concurrently npm:build:lambda npm:build:web",
    "build:lambda": "netlify-lambda build -c webpack.functions.js src/lambda",
    "build:provisioner": "webpack-cli -c webpack.provisioner.js",
    "build:web": "webpack-cli -c webpack.web.js",
    "build:graphql": "graphql-codegen",
    "develop": "yarn pm2 start ./ecosystem.config.js",
    "develop:stop": "yarn pm2 stop ./ecosystem.config.js",
    "test": "jest",
    "n": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baublet/quick-dev.git"
  },
  "author": "Ryan Poe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baublet/quick-dev/issues"
  },
  "homepage": "https://github.com/baublet/quick-dev#readme",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "apollo-server-lambda": "^2.19.0",
    "classnames": "^2.2.6",
    "cookie": "^0.4.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.13",
    "glob": "^7.1.6",
    "human-readable-ids": "^1.0.4",
    "js-yaml": "^4.0.0",
    "knex": "^0.21.14",
    "memory-fs": "^0.5.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.0.0-beta.9",
    "node-pty": "^0.9.0",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.0",
    "ssh-keygen": "^0.5.0",
    "sshpk": "^1.16.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/jest": "^26.0.19",
    "@types/js-yaml": "^3.12.5",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "netlify-cli": "^2.69.11",
    "netlify-lambda": "^2.0.2",
    "ngrok": "^3.4.0",
    "pm2": "^4.5.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
