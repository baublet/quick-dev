  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baublet/quick-dev/issues"
  },
  "homepage": "https://github.com/baublet/quick-dev#readme",
  "dependencies": {
    "apollo-server-lambda": "^2.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "knex": "^0.21.14",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^4.5.0",
    "netlify-cli": "^2.69.11",
    "netlify-lambda": "^2.0.2",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
